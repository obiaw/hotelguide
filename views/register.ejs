  <%-contentFor('booking') %>

  <div class="container" style="margin-top: 150px;">
  	<div class="row login">
  		<div class="col-sm-6 mx-auto" style="background: #fff;padding: 10px;">
  			<div class="form-box" style="top: 143.5px;">
			<div class="form-head">
				<div class="mold-logo">
					<div class="logo-txt text-center" style="font-size: 2.2em;">HotelGuideBot Create Account</div>
				</div>
			</div>
			<div class="form-body justify-content-center">
				<form>
					<div class="form-group">
						<label>Hotel Name</label>
						<div class="input-group">
							<div class="input-group-addon fa fa-home"></div>
							<input class="form-control" placeholder="Hotel Name eg. Serena Hotel" type="text">
						</div>
					</div>
					<div class="form-group">
						<label>Email Address</label>
						<div class="input-group">
							<div class="input-group-addon fa fa-envelope"></div>
							<input class="form-control" placeholder="Email Address" type="text">
						</div>
					</div>

					<div class="form-group">
						<label>Password</label>
						<div class="input-group">
							<div class="input-group-addon fa fa-lock"></div>
							<input class="form-control" placeholder="Password" type="password">
						</div>
					</div>
					<button type="submit" class="btn btn-primary hvr-sweep-to-right">Account LogIn</button>
					<!-- <button type="submit" class="btn btn-primary hvr-sweep-to-right">Account LogIn</button> -->
					&nbsp;<a href="/admin/login"> Have an account?</a>
				</form>
			</div>
		</div>
  		</div>
  	</div>
  </div>